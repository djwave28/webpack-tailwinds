  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "try": "webpack open --config webpack.config.js",
    "watch": "webpack --watch --env mode=development ",
    "build": "webpack --env mode=production --progress",
    "prod": "webpack --env --env mode=production --progress",
    "production": "webpack --env mode production --progress --env config production",
    "build": "webpack --env mode development --progress --env config production",
    "watch": "webpack-dev-server --env mode development --env config development --open --watch --info-verbosity verbose --progress",
    "start": "webpack-dev-server --env mode development --env config development --open --watch",
    "quiet": "webpack-dev-server --env mode development --env config development --open --watch --env.stats none",
    "verbose": "webpack-dev-server --env mode development --env config development --open --watch --env stats verbose"
  },
